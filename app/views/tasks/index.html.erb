<div class="todo-header">
  <h2>ToDo List</h2>
  <img src="<%= asset_path('notebook.gif') %>" height="50px" />
</div>

<div class="todo-body">
  <input
    type="text"
    id="todoText"
    class="todo-input"
    placeholder="Add your items"
  />
  <img
    src="<%= asset_path('plus.png') %>"
    alt="+"
    id="AddUpdateClick"
    class="add-icon"
  />
</div>

<div id="list-items"></div>
